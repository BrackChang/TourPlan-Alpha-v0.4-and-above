<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#EB8887" android:endColor="#E76E6D" android:angle="90"/>
            <stroke android:width="1dip" android:color="#cccccc" />
            <corners android:radius="3dp" />
        </shape>
    </item>
    
    <item>
        <shape>
            <gradient android:startColor="@drawable/Tan" android:endColor="@drawable/Tan" android:angle="90" />
            <stroke android:width="1dip" android:color="@drawable/Transparent2" />
            <corners android:radius="3dip" />
        </shape>
    </item>

</selector>